{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "2.1.6",
    "verbb/super-table": "2.0.10",
    "barrelstrength/sprout-fields": "v3.1.9",
    "craftcms/contact-form": "2.2.2",
    "fortrabbit/craft-copy": "^1.0.0-beta",
    "aelvan/mailchimp-subscribe": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
